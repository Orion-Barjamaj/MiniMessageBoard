<html>
    <head>
        <title>Message <%= message.id %></title>
        <link rel="stylesheet" href="/style.css">
    </head>
    <body>
        <%- include('navbar') %>
        <div class="messageHandle">
            <div class="infoHandle">
                <div class="messageName"><%= message.user %></div>
                <div>Posted on: <%= message.added %></div>
            </div>
            <div class="textHandle"><%= message.text %></div>
        </div>
    </body>
</html>